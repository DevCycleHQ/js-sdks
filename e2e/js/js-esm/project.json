{
    "name": "js-esm",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "e2e/js/js-esm/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/e2e/js/js-esm",
                "compiler": "swc",
                "main": "e2e/js/js-esm/src/main.ts",
                "package": "e2e/js/js-esm/package.json",
                "tsConfig": "e2e/js/js-esm/tsconfig.app.json",
                "webpackConfig": "e2e/js/js-esm/webpack.config.js",
                "assets": [
                    "e2e/js/js-esm/src/favicon.ico",
                    "e2e/js/js-esm/src/assets"
                ],
                "index": "e2e/js/js-esm/src/index.html",
                "baseHref": "/",
                "styles": ["e2e/js/js-esm/src/styles.css"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false,
                    "fileReplacements": [
                        {
                            "replace": "e2e/js/js-esm/src/environments/environment.ts",
                            "with": "e2e/js/js-esm/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve": {
            "executor": "@nx/webpack:dev-server",
            "options": {
                "buildTarget": "js-esm:build"
            },
            "configurations": {
                "production": {
                    "buildTarget": "js-esm:build:production"
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["e2e/js/js-esm/**/*.ts"]
            }
        },
        "e2e": {
            "executor": "@nx/playwright:playwright",
            "outputs": ["{workspaceRoot}/dist/.playwright/e2e/js/js-esm"],
            "options": {
                "config": "e2e/js/js-esm/playwright.config.ts"
            }
        }
    }
}
