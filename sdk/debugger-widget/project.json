{
    "name": "sdk-debugger-widget",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "sdk/debugger-widget/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nx/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/sdk/debugger-widget",
                "index": "sdk/debugger-widget/src/index.html",
                "baseHref": "/",
                "main": "sdk/debugger-widget/src/main.tsx",
                "tsConfig": "sdk/debugger-widget/tsconfig.app.json",
                "assets": [
                    "sdk/debugger-widget/src/favicon.ico",
                    "sdk/debugger-widget/src/assets"
                ],
                "styles": ["sdk/debugger-widget/src/styles.css"],
                "scripts": [],
                "isolatedConfig": true,
                "webpackConfig": "sdk/debugger-widget/webpack.config.js",
                "postcssConfig": "sdk/debugger-widget/postcss.config.js"
            },
            "configurations": {
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                },
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "sdk/debugger-widget/src/environments/environment.ts",
                            "with": "sdk/debugger-widget/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nx/webpack:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "sdk-debugger-widget:build",
                "hmr": true,
                "port": 4201
            },
            "configurations": {
                "development": {
                    "buildTarget": "sdk-debugger-widget:build:development"
                },
                "production": {
                    "buildTarget": "sdk-debugger-widget:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["sdk/debugger-widget/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "sdk/debugger-widget/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": []
}
