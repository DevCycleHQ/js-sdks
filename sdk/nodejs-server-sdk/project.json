{
  "root": "sdk/nodejs-server-sdk",
  "sourceRoot": "sdk/nodejs-server-sdk/src",
  "targets": {
    "check-types": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn run -T tsc -b --incremental",
        "cwd": "sdk/nodejs-server-sdk"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "sdk/nodejs-server-sdk/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/sdk/nodejs-server-sdk"
      ],
      "options": {
        "jestConfig": "sdk/nodejs-server-sdk/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/sdk/nodejs-server-sdk",
        "tsConfig": "sdk/nodejs-server-sdk/tsconfig.lib.json",
        "packageJson": "sdk/nodejs-server-sdk/package.json",
        "main": "sdk/nodejs-server-sdk/src/index.ts",
        "assets": [
          "sdk/nodejs-server-sdk/*.md"
        ]
      }
    },
    "npm-publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "dependsOn": [
          "build"
        ],
        "command": "../../scripts/npm-publish-check.sh \"@devcycle/nodejs-server-sdk\"",
        "cwd": "sdk/js"
      }
    }
  },
  "tags": []
}
