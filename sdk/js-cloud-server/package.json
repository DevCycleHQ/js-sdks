{
    "name": "@devcycle/js-cloud-server-sdk",
    "version": "1.25.1",
    "description": "The DevCycle JS Cloud Bucketing Server SDK used for feature management.",
    "author": "DevCycle <support@devcycle.com>",
    "keywords": [
        "devcycle",
        "feature flag",
        "javascript",
        "client",
        "serverless",
        "cloud",
        "sdk"
    ],
    "repository": {
        "type": "git",
