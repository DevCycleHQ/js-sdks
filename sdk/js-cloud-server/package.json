    },
    "license": "MIT",
    "homepage": "https://devcycle.com",
    "dependencies": {
        "@devcycle/types": "^1.26.1",
        "cross-fetch": "^4.1.0",
        "fetch-retry": "^5.0.6",
        "lodash": "^4.17.21"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "main": "./index.cjs",
    "module": "./index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./index.d.ts",
                "default": "./index.cjs"
