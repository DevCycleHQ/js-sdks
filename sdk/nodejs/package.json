{
    "name": "@devcycle/nodejs-server-sdk",
    "version": "1.46.1",
    "description": "The DevCycle NodeJS Server SDK used for feature management.",
    "license": "MIT",
    "author": "DevCycle <support@devcycle.com>",
    "keywords": [
        "devcycle",
        "feature flag",
        "javascript",
        "client",
        "openfeature",
        "node",
        "server",
        "sdk"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/DevCycleHQ/js-sdks.git"
    },
    "homepage": "https://devcycle.com",
    "dependencies": {
        "@devcycle/bucketing-assembly-script": "^1.35.1",
        "@devcycle/js-cloud-server-sdk": "^1.25.1",
        "@devcycle/types": "^1.26.1",
        "@openfeature/core": "^1.7.2",
        "@openfeature/server-sdk": "^1.17.1",
        "cross-fetch": "^4.1.0",
        "eventsource": "^2.0.2",
        "fetch-retry": "^5.0.6"
    },
    "peerDependencies": {
        "@devcycle/js-client-sdk": "^1.36.2",
        "@openfeature/multi-provider": "^0.1.2"
    },
    "peerDependenciesMeta": {
        "@devcycle/js-client-sdk": {
            "optional": true
        },
        "@openfeature/multi-provider": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "main": "./src/index.cjs",
    "module": "./src/index.js",
    "types": "./src/index.d.ts",
    "exports": {
        ".": {
            "require": {
                "types": "./src/index.d.ts",
                "default": "./src/index.cjs"
            },
            "import": {
                "types": "./src/index.d.ts",
                "default": "./src/index.js"
            }
        },
        "./openfeature-strategy": {
            "require": {
                "types": "./openfeature-strategy.d.ts",
                "default": "./openfeature-strategy.cjs"
            },
            "import": {
                "types": "./openfeature-strategy.d.ts",
                "default": "./openfeature-strategy.js"
            }
        }
    }
}
