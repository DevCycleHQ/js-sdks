{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "examples/nodejs/typescript/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/examples/nodejs/typescript",
        "main": "examples/nodejs/typescript/src/main.ts",
        "tsConfig": "examples/nodejs/typescript/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "example-nodejs-typescript:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "examples/nodejs/typescript/**/*.ts"
        ]
      }
    }
  },
  "tags": []
}
