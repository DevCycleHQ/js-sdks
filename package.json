{
  "private": true,
  "name": "devcycle-js-sdks",
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "lib/**/*",
      "sdk/*"
    ]
  },
  "scripts": {
    "lint": "nx run-many --parallel --target lint --all",
    "test": "nx run-many --parallel --target test --all",
    "lerna": "lerna",
    "lerna:test:unit": "lerna run unit-test",
    "lerna:version": "lerna version -m \"Release\"",
    "lerna:version:force": "lerna version -m \"Release\" --force-publish",
    "lerna:version:force:minor": "lerna version minor -m \"Release\" --force-publish",
    "lerna:version:patch": "lerna version patch -m \"Release\"",
    "lerna:version:minor": "lerna version minor -m \"Release\"",
    "lerna:version:major": "lerna version major -m \"Release\""
  },
  "dependencies": {
    "async": "^3.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.1",
    "jira-prepare-commit-msg": "^1.6.2",
    "lint-staged": "^11.2.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "murmurhash": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^4.2.3",
    "tslib": "^2.0.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nrwl/cli": "13.1.3",
    "@nrwl/devkit": "13.1.3",
    "@nrwl/eslint-plugin-nx": "13.1.3",
    "@nrwl/jest": "13.1.3",
    "@nrwl/linter": "13.1.3",
    "@nrwl/node": "13.1.3",
    "@nrwl/tao": "13.1.3",
    "@nrwl/workspace": "13.1.3",
    "@types/async": "^3.2.8",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^17.0.21",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "babel-jest": "27.2.3",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "jest": "27.2.3",
    "jest-cli": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "6.4.0",
    "lerna": "^3.18.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "prettier-eslint": "~12.0.0",
    "prettier-eslint-cli": "~5.0.1",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "resolutions": {},
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/pre_commit.sh",
      "prepare-commit-msg": "./scripts/prep_commit_msg.sh"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "nx affected --uncommitted --parallel --target=check-types && :",
      "nx affected:lint --uncommitted --parallel --fix=true && :"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
