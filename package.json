    "affected:test": "nx affected --base=origin/main --head=HEAD --target=test",
    "npm-publish": "nx affected --base=origin/main~1 --target=npm-publish",
    "prettier:format": "yarn prettier --write .",
    "start": "nx serve",
    "lerna:version": "scripts/lerna-version.sh",
