{
  "private": true,
  "name": "devcycle-js-sdks",
  "version": "1.0.0",
  "engines": {
    "node": "^16.13"
  },
  "workspaces": [
    "sdk/*",
    "lib/**/*",
    "example/**/*"
  ],
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "lint": "nx run-many --parallel --target lint --all",
    "test": "nx run-many --parallel --target test --all",
    "affected:lint": "npx nx affected --base=origin/main --target=lint",
    "affected:test": "npx nx affected --base=origin/main --target=test --codeCoverage",
    "npm-publish": "npx nx affected --base=origin/main~1 --target=npm-publish",
    "start": "npx nx serve",
    "lerna": "lerna",
    "lerna:test:unit": "lerna run unit-test",
    "lerna:version": "lerna version -m \"Release\"",
    "lerna:version:force": "lerna version -m \"Release\" --force-publish",
    "lerna:version:force:minor": "lerna version minor -m \"Release\" --force-publish",
    "lerna:version:patch": "lerna version patch -m \"Release\"",
    "lerna:version:minor": "lerna version minor -m \"Release\"",
    "lerna:version:major": "lerna version major -m \"Release\""
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.19.23",
    "assemblyscript": "^0.19.23",
    "assemblyscript-json": "^1.1.0",
    "@nrwl/next": "13.8.5",
    "async": "^3.2.1",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.1",
    "core-js": "^3.6.5",
    "jira-prepare-commit-msg": "^1.6.2",
    "lint-staged": "^11.2.3",
    "lodash": "^4.17.21",
    "md5": "^2.2.1",
    "moment": "^2.29.1",
    "murmurhash": "^2.0.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^4.2.3",
    "tslib": "^2.0.0",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@nrwl/cli": "13.8.8",
    "@nrwl/devkit": "13.8.8",
    "@nrwl/eslint-plugin-nx": "13.8.8",
    "@nrwl/jest": "13.8.8",
    "@nrwl/js": "13.8.8",
    "@nrwl/linter": "13.8.8",
    "@nrwl/next": "13.8.8",
    "@nrwl/node": "13.8.8",
    "@nrwl/react": "13.8.8",
    "@nrwl/tao": "13.8.8",
    "@nrwl/web": "13.8.8",
    "@nrwl/workspace": "13.8.8",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/async": "^3.2.8",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "babel-jest": "27.2.3",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^7.0.3",
    "eslint": "8.7.0",
    "eslint-config-next": "12.1.1-canary.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "jest": "27.2.3",
    "jest-cli": "^27.0.6",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "6.4.0",
    "lerna": "^3.18.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "prettier-eslint": "~12.0.0",
    "prettier-eslint-cli": "~5.0.1",
    "react": "^17.0.2",
    "react-test-renderer": "17.0.2",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  },
  "resolutions": {},
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/pre_commit.sh",
      "prepare-commit-msg": "./scripts/prep_commit_msg.sh"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "nx affected --uncommitted --parallel --target=check-types && :",
      "nx affected:lint --uncommitted --parallel --fix=true && :"
    ]
  }
}

