{
    "root": "lib/shared/bucketing-assembly-script",
    "sourceRoot": "lib/shared/bucketing-assembly-script/assembly",
    "projectType": "library",
    "implicitDependencies": ["shared-bucketing-test-data"],
    "targets": {
        "test": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": [
                    {
                        "command": "node --experimental-vm-modules ../../../node_modules/jest/bin/jest.js"
                    }
                ],
                "cwd": "lib/shared/bucketing-assembly-script"
            },
            "dependsOn": [
                {
                    "target": "build",
                    "projects": "self"
                },
                {
                    "target": "build:json",
                    "projects": "dependencies"
                }
            ]
        },
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": [
                    {
                      "command": "asc assembly/index.ts --target debug --exportRuntime --initialMemory 10000 --maximumMemory 50000 --bindings esm"
                    },
                    {
                      "command": "asc assembly/index.ts --target release --exportRuntime --initialMemory 10000 --maximumMemory 50000 --bindings esm"
                    }
                ],
                "cwd": "lib/shared/bucketing-assembly-script",
                "parallel": true
            }
        }
    },
    "tags": []
}
