{
    "root": "lib/shared/bucketing-assembly-script",
    "sourceRoot": "lib/shared/bucketing-assembly-script/assembly",
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/lib/shared/bucketing-assembly-script"
            ],
            "dependsOn": [
                {
                    "target": "build",
                    "projects": "self"
                }
            ],
            "options": {
                "jestConfig": "lib/shared/bucketing-assembly-script/jest.config.js"
            }
        },
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": [
                    {
                      "command": "asc assembly/index.ts --target debug --exportRuntime --initialMemory 100 --maximumMemory 10000 --bindings esm"
                    },
                    {
                      "command": "asc assembly/index.ts --target release --exportRuntime --initialMemory 100 --maximumMemory 10000 --bindings esm"
                    }
                ],
                "cwd": "lib/shared/bucketing-assembly-script",
                "parallel": true
            }
        }
    },
    "tags": []
}
