{
    "root": "lib/shared/bucketing-assembly-script",
    "sourceRoot": "lib/shared/bucketing-assembly-script/src",
    "projectType": "library",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/lib/shared/bucketing-assembly-script"
            ],
            "dependsOn": [
                {
                    "target": "build",
                    "projects": "self"
                }
            ],
            "options": {
                "jestConfig": "lib/shared/bucketing-assembly-script/jest.config.js",
                "passWithNoTests": true
            }
        },
        "build": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "commands": [
                    { "command": "asc src/index.ts --target debug --exportRuntime" },
                    { "command": "asc src/index.ts --target release --exportRuntime" }
                ],
                "cwd": "lib/shared/bucketing-assembly-script",
                "parallel": true
            }
        }
    },
    "tags": []
}
