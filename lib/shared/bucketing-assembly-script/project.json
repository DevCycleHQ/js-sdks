{
  "root": "lib/shared/bucketing-assembly-script",
  "sourceRoot": "lib/shared/bucketing-assembly-script/assembly-script-src",
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/lib/shared/bucketing-assembly-script"
      ],
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "options": {
        "jestConfig": "lib/shared/bucketing-assembly-script/jest.config.js"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "asc assembly-script-src/index.ts --target debug --exportRuntime --initialMemory 100 --maximumMemory 10000"
          },
          {
            "command": "asc assembly-script-src/index.ts --target release --exportRuntime --initialMemory 100 --maximumMemory 10000"
          }
        ],
        "cwd": "lib/shared/bucketing-assembly-script",
        "parallel": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "commitMessageFormat": "chore(${projectName}): release version ${version}"
      }
    }
  },
  "tags": []
}
