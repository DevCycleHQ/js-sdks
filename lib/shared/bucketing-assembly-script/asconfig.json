{
  "targets": {
    "debug": {
      "outFile": "build/bucketing-lib.debug.wasm",
      "textFile": "build/bucketing-lib.debug.wat",
      "tsdFile": "build/bucketing-lib.debug.d.ts",
      "debug": true
    },
    "release": {
      "outFile": "build/bucketing-lib.release.wasm",
      "textFile": "build/bucketing-lib.release.wat",
      "tsdFile": "build/bucketing-lib.release.d.ts",
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "optimize": true,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "initialMemory": 10000,
    "noColors": true,
    "exportRuntime": true,
    "bindings": "esm",
    "stats": true,
    "sourceMap": true
  }
}
