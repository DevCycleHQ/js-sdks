{
    "name": "shared-bucketing-test-data",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "lib/shared/bucketing-test-data/src",
    "projectType": "library",
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["lib/shared/bucketing-test-data/**/*.ts"]
            }
        },
        "build": {
            "executor": "@nx/esbuild:esbuild",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/lib/shared/bucketing-test-data",
                "tsConfig": "lib/shared/bucketing-test-data/tsconfig.lib.json",
                "main": "lib/shared/bucketing-test-data/src/main.ts",
                "format": ["cjs", "esm"],
                "platform": "node",
                "bundle": true,
                "declaration": true,
                "sourcemap": true,
                "assets": ["lib/shared/bucketing-test-data/package.json"],
                "external": [
                    "@devcycle/types",
                    "class-transformer",
                    "class-validator",
                    "reflect-metadata"
                ]
            },
            "dependsOn": [
                {
                    "target": "build",
                    "dependencies": true
                }
            ]
        },
        "build:json": {
            "executor": "@nx/js:node",
            "options": {
                "buildTarget": "shared-bucketing-test-data:build",
                "watch": false
            },
            "dependsOn": [
                {
                    "target": "build",
                    "dependencies": true
                }
            ]
        }
    },
    "tags": []
}
