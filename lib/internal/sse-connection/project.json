{
    "name": "sse-connection",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "lib/internal/sse-connection/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/lib/internal/sse-connection",
                "main": "lib/internal/sse-connection/src/index.ts",
                "tsConfig": "lib/internal/sse-connection/tsconfig.lib.json",
                "assets": ["lib/internal/sse-connection/*.md"],
                "external": ["@devcycle/types"]
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["lib/internal/sse-connection/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "lib/internal/sse-connection/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": []
}
